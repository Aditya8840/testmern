{
     
    "builds": [
        {
            "src": "./3request.js",
            "use": "@vercel/node"
        },{
            "src": "build/",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/quotes",
            "dest": "3request.js"
        },{
            "src": "/api/quotes/(.*)",
            "dest": "3request.js"
        },{
            "src": "/(.*)",
            "dest": "build/**"
        },{
            "src": "/",
            "dest": "build/index.html"
        },{
            "src": "/(.+)",
            "dest": "build/$1"
        }
    ]
}